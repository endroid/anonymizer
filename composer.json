{
    "name": "endroid/anonymizer",
    "description": "Endroid Anonymizer",
    "keywords": ["endroid", "anonymizer", "anonymize", "sensitive", "php"],
    "homepage": "https://github.com/endroid/anonymizer",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeroen van den Enden",
            "email": "info@endroid.nl"
        }
    ],
    "require": {
        "php": ">=7.4",
        "doctrine/common": "^2.0||^3.0",
        "endroid/installer": "^1.2",
        "fzaninotto/faker": "^1.0",
        "symfony/event-dispatcher": "^5.1"
    },
    "require-dev": {
        "endroid/quality": "dev-master"
    },
    "suggest": {
        "roave/security-advisories": "Avoids installation of package versions with vulnerabilities",
        "symfony/security-checker": "Checks your composer.lock for vulnerabilities"
    },
    "autoload": {
        "psr-4": {
            "Endroid\\Anonymizer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Endroid\\Anonymizer\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
